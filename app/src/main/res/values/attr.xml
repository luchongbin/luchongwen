<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MainNavigateTabBar">
        <attr name="navigateTabTextColor" format="color|reference" />
        <attr name="navigateTabSelectedTextColor" format="color|reference" />
        <attr name="containerId" format="integer|reference" />
        <attr name="navigateTabTextSize" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="TopBarView">
        <attr name="titleText" format="string|reference" />
        <attr name="barBackground" format="reference" />
        <attr name="hasBack" format="boolean" />
    </declare-styleable>
    <!-- 圆形图片 -->
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>
</resources>